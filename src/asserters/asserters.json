{
  "DirectoryExists": {
    "module": "DirectoryExistsAsserter",
    "arguments": ["path!"],
    "remotable": true,
    "description": "Asserts that a directory exists. ",
    "dependencies": {
      "fs-extra": "^7.0.1"
    }
  },
  "DirectoryAbsent": {
    "module": "DirectoryAbsentAsserter",
    "arguments": ["path!"],
    "remotable": true,
    "description": "Asserts that a directory does not exist",
    "dependencies": {
      "fs-extra": "^7.0.1"
    }
  },
  "FileExists": {
    "module": "FileExistsAsserter",
    "arguments": ["path!"],
    "remotable": true,
    "description": "Asserts that a file exists. If not write a placeholder out",
    "dependencies": {
      "fs-extra": "^7.0.1"
    }
  },
  "FileAbsent": {
    "module": "FileAbsentAsserter",
    "arguments": ["path!"],
    "remotable": true,
    "description": "Asserts that a file does not exist. ",
    "dependencies": {
      "fs-extra": "^7.0.1"
    }
  },
  "FileDownloaded": {
    "module": "FileDownloadedAsserter",
    "arguments": ["url!", "checksum!", "checksumType", "toFile!"],
    "remotable": true,
    "description": "Asserts that a file as been downloaded. ",
    "dependencies": {
      "fs-extra":"^7.0.1",
      "request": "^2.0.0"
    }
  },
  "FileUnzipped": {
    "module": "FileUnzippedAsserter",
    "arguments": ["zipFile!", "toDir!"],
    "remotable": true,
    "description": "Asserts that a file has been unzipped. ",
    "dependencies": {
      "fs-extra":"^7.0.1",
      "unzip": "^0.1.11"
    }
  }

}